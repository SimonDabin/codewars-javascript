{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": "off",
        "max-len":"off",
    },
    "plugins": ["jest"]

}
